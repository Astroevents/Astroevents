
<% if (user) { %>
  <div>
    <h2>Wellcome <%= user.username %> </h2>
      <a href="/logout">logout</a>
  </div>
<% } %>


<ul>
  <% events.forEach((event) => { %>
    <li>
      <%= event.name %> - <%= event.description %>
    </li>
    <img src="<%= event.imageUrl %>" alt="">
      </img>
      <a href="/events/<%= event._id %>">View Details</a>
  <%  }) %>
</ul>

<a href="/new">new event</a>
