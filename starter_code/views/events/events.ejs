<div class="container">

<% if (user) { %>
  <div>
    <h2>Welcome <%= user.username %> </h2>
  </div>
<% } %>

<div class="container">
<div class="">

<ul>
  <% events.forEach((event) => { %>
    <li>
      <%= event.name %> - <%= event.description %>
    </li>
    <img src="<%= event.imageUrl %>" alt="">
      <a href="/events/<%= event._id %>">View Details</a>
      <a href="/events/<%= event._id %>/edit/">Edit event</a>
      <li>
        <form action="/events/<%= event._id%>/delete"&&"/comments/<%= event._id%>/delete" method="POST">
          <button type="submit">Delete Event</button>
        </form>
      </li>

  <%  }); %>
</ul>
</div>
</div>



<a href="/logout">logout</a>
<br>
<a href="/new">new event</a>
</div>
