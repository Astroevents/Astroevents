<form action="/events/<%= event._id %>" method="POST">
  <label for="name">Name</label>
  <input type="text" id="name" name="name" value="<%= event.name %>">
<br></br>
  <label for="place">place</label>
  <input type="text" name="place" id="place" value="<%= event.place %>">
<br></br>
  <label for="city">city</label>
  <input type="text" name="city" id="city" value="<%= event.city %>">
<br></br>
  <label for="date">date</label>
  <input type="date" name="date" id="date" value="<%= event.date %>">
<br></br>
  <label for="imageUrl">Image URL</label>
  <input type="url" name="imageUrl" id="imageUrl" value="<%= event.imageURL %>">
<br></br>
  <label for="description">Description</label>
  <textarea type="text" name="description" id="description" value="<%= event.description %>"></textarea>
<br></br>
  <label for="category">category</label>
  <input type="text" name="category" id="category" value="<%= event.description %>">
  <br></br>
  <button type="submit">Update Event</button>
</form>
